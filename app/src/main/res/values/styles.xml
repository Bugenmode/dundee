<resources>

	<style name="Dundee" parent="Theme.AppCompat.Light.NoActionBar">
		<item name="colorPrimary">@color/primary</item>
		<item name="colorPrimaryDark">@color/primaryDark</item>
		<item name="colorAccent">@color/primary</item>
		<item name="android:windowBackground">@color/background</item>
		<item name="android:windowLightStatusBar">true</item>
	</style>

	<style name="ThemeOverlay.Dundee.Dark" parent="ThemeOverlay.AppCompat.Dark">
		<item name="colorAccent">@color/white</item>
	</style>



	<style name="Theme.Dundee.Picker" parent="@style/Theme.AppCompat.Light">
		<item name="android:textColorPrimary">@color/text_default</item>
		<item name="android:textSize">@dimen/date_picker_text</item>
		<item name="colorControlNormal">@android:color/transparent</item>
	</style>
</resources>
